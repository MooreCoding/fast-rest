    }
  ],
  "require": {
    "php": ">=5.5.0",
    "ext-phalcon": ">=2.0.4 <3.0",
    "zircote/swagger-php": "*"
  },
  "require-dev": {
    "phalcon/incubator": "dev-master",
	"phpunit/phpunit": "5.0.*",
	"phpunit/php-code-coverage": "dev-master#b8436b000263f6d72fbad1d36890e247ce84857e",
    "codeclimate/php-test-reporter": "dev-master"
